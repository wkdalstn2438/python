"""
def print_5xn(a):
    c = 0
    for i in a:
        if c % 5 == 0 and c  != 0:
            print()
        print(i, end='')
        c += 1
print_5xn('아이엠어보이유알어걸')
"""
"""
def print_5xn(a, b):
    c = 0
    for i in a:
        if c % b == 0 and c  != 0:
            print()
        print(i, end='')
        c += 1
print_5xn('아이엠어보이유알어걸', 3)
"""
def calc_monthly_salary(a):
    b = int(a // 12)
    return b



a = calc_monthly_salary(120000)
print(a)


