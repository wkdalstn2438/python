"""
def print_max(a, b, c):
    if a > b and a > c:
        print(a)
    elif b > a and b > c:
        print(b)
    elif c > b and c > a:
        print(c)

print_max(9, 7, 5)

def print_max(a, b, c):
    f = 0
    if a > f:
        f = a
    if b > f:
        f = b
    if c > f:
        f = c
    print(f)
print_max(9, 7, 5)
"""
#221
"""
def print_reverse(a):
    b = ''
    for i in a:
        b = i + b
    return b

a = print_reverse('python')
print(a)
"""
#222
"""
def print_score(list):
    return (sum(list)/len(list))

a = print_score([1, 2, 3])
print(a)
"""
#223
"""
def print_even(list):
    for i in list:
        if i % 2 == 0:
            print(i)

a = print_even([1, 3, 2, 10, 12, 11, 15])
"""
